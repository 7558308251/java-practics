import java.util.*;
class mop
{
	void show()
	{
		Scanner op=new Scanner(System.in);
		int a,i,j;
		System.out.println("enter the number=");
		a=op.nextInt();
		for(i=0;i<=a;i++){
			for(j=1;j<=i;j++){
				System.out.print("* ");
			}
			System.out.println();
		}
	}
}
class pater
{
	public static void main(String args[])
	{
		mop ob=new mop();
		ob.show();
	}
}
